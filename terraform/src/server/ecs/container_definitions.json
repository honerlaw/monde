[
  {
    "name": "server",
    "essential": true,
    "image": "${image}",
    "portMappings": [
      {
        "containerPort": ${container_port},
        "hostPort": ${container_port}
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "awslogs-vueon",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "server"
      }
    }
  }
]